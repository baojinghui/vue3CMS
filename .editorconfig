#  https://EditorConfig.org
root = true #.editorConfig配置在根目录
[*] #表示所有文件适用
charset = utf-8 #设置文件字符集为utf-8
indent_style = space #缩进风格(tab|tab)
indent_size = 2 #缩进大小
end_of_line = lf #强制换行类型
trim_trailing_whitespace = true #去除首行的任意空白符
insert_final_newline = true #始终在文件末尾插入新行

[*.md] # 只在。md文件中适用以下规则
max_line_length=off
trim_trailing_whitespace = false


